{
    "workbench.editor.enablePreview": false,
    "editor.cursorStyle": "block",
    "editor.cursorBlinking": "solid",
    "workbench.colorTheme": "Night Owl (No Italics)",
    "workbench.editor.untitled.hint": "hidden",
    "editor.occurrencesHighlight": false,
    "editor.renderWhitespace": "none",
    "editor.wordWrap": "wordWrapColumn",
    "files.eol": "\n",
    "editor.rulers": [
        100
    ],
    "workbench.colorCustomizations": {
        "editorCursor.foreground": "#8c9600",
        "terminalCursor.foreground": "#eeff00",
        "editorRuler.foreground": "#797979"
    },
    "git.confirmSync": false,
    "outline.showVariables": false,
    "workbench.editor.wrapTabs": true,
    "breadcrumbs.showVariables": false,
    "redhat.telemetry.enabled": false,
    "security.workspace.trust.startupPrompt": "never",
    "workbench.startupEditor": "none",
    "window.titleBarStyle": "custom",
    "telemetry.telemetryLevel": "off",
    "bracket-pair-colorizer-2.showHorizontalScopeLine": false,
    "bracket-pair-colorizer-2.showVerticalScopeLine": false,
    "files.trimTrailingWhitespace": true,
    "metaGo.decoration.characters": "a,s,d,f,g,h,j,k,l",
    "metaGo.decoration.additionalSingleCharCodeCharacters": "F,J",
    "window.zoomLevel": -1,
    "jupyter.interactiveWindowMode": "perFile",
    "jupyter.askForKernelRestart": false,
    "http.proxyStrictSSL": false,
    "todohighlight.isEnable": true,
    "center-editor-window.threeStateToggle": true,
    "center-editor-window.offset": 5,
    "workbench.iconTheme": "vscode-icons",
    "editor.wordWrapColumn": 100,
    "security.workspace.trust.untrustedFiles": "open",
    "todohighlight.keywords": [
        {
            "text": "TODO",
            "color": "white",
            "backgroundColor": "#16c60cff",
            "before": {
			    "contentText": "✅"
			},
        },
        {
            "text": "WARN",
            "color": "black",
            "backgroundColor": "yellow",
            "before": {
			    "contentText": "⚠️"
			},
        }
    ],
    "todohighlight.defaultStyle": {
        "color": "black",
        "border": "1px solid black",
        "borderRadius": "5px"
    },
}
